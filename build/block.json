{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ingeni-structural/ingeni-structural-block",
  "version": "0.1.0",
  "title": "Ingeni Structural Block",
  "category": "layout",
  "icon": "hammer",
  "description": "Provides additional HTML structural elements for implement background images and pseudo element images.",
  "supports": {
    "html": true,
    "align": true
  },
  "textdomain": "ingeni-structural-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "backgroundColor": {
      "type": "string"
    },
    "customBackgroundColor": {
      "type": "string"
    },
    "bgImage": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src",
      "default": ""
    },
    "beforeImage": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src",
      "default": ""
    },
    "afterImage": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src",
      "default": ""
    }
  }
}